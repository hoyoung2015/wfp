df = read.csv('clustor_data.csv',colClasses = c('stockCode'='character'))
# View(df[,2:6])
data=df[,3:7]
dist.e=dist(data,method='euclidean')
# heatmap(as.matrix(dist.e),labRow = F, labCol = F)
model1=hclust(dist.e,method='ward.D2')
result=cutree(model1,k=5)
df['result'] = result
write.csv(df,file = 'clustor_result_data.csv',row.names = F)
mds=cmdscale(dist.e,k=2,eig=T)
x = mds$points[,1]
y = mds$points[,2]
library(ggplot2)
p=ggplot(data.frame(x,y),aes(x,y))
p+geom_point(size=3,alpha=0.8,
aes(colour=factor(result),
shape='versicolor'))
